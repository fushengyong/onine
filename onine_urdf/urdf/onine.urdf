<?xml version="1.0"?>
<robot name="onine">

    <material name="orange">
        <color rgba="0.57 0.17 0.0 1.0" />
    </material>

    <material name="white">
        <color rgba="0.8 0.8 0.8 0.50" />
    </material>

    <material name="steel">
        <color rgba="0.75 0.75 0.75 0.30" />
    </material>

    <material name="gray">
        <color rgba="0.2 0.2 0.2 0.9" />
    </material>

    <material name="black">
        <color rgba="0.0 0.0 0.0 1.0" />
    </material>

    <link name="base_footprint" />

    <link name="wheel1">
        <visual>
            <geometry>
                <cylinder length="0.05" radius=".05" />
            </geometry>
            <material name="gray" />
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="wheel1_joint" type="fixed">
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="0.11 0.17 0.05" />
        <parent link="base_footprint" />
        <child link="wheel1" />
    </joint>

    <link name="wheel2">
        <visual>
            <geometry>
                <cylinder length="0.05" radius=".05" />
            </geometry>
            <material name="gray" />
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="wheel2_joint" type="fixed">
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="0.11 -0.17 0.05" />
        <parent link="base_footprint" />
        <child link="wheel2" />
    </joint>

    <link name="wheel3">
        <visual>
            <geometry>
                <cylinder length="0.05" radius=".05" />
            </geometry>
            <material name="gray" />
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="wheel3_joint" type="fixed">
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="-0.11 0.17 0.05" />
        <parent link="base_footprint" />
        <child link="wheel3" />
    </joint>

    <link name="wheel4">
        <visual>
            <geometry>
                <cylinder length="0.05" radius=".05" />
            </geometry>
            <material name="gray" />
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="wheel4_joint" type="fixed">
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="-0.11 -0.17 0.05" />
        <parent link="base_footprint" />
        <child link="wheel4" />
    </joint>

    <link name="camera_link">
        <visual>
            <geometry>
                <box size="0.06 0.28 0.035" />
            </geometry>
            <material name="black" />
            <origin rpy="0 0 0" xyz="0 -0.03 0" />
        </visual>
    </link>

    <joint name="camera_link_joint" type="fixed">
        <axis xyz="0 0 1" />
        <origin rpy="0 0.593412 0" xyz="-0.001 0.03 0.5675" />
        <parent link="vertical_link" />
        <child link="camera_link" />
    </joint>

    <link name="beam_link">
        <visual>
            <geometry>
                <box size="0.02 0.07 0.025" />
            </geometry>
            <material name="black" />
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="beam_link_joint" type="fixed">
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="0.021 0 0.3975" />
        <parent link="vertical_link" />
        <child link="beam_link" />
    </joint>

    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.33 0.23 0.15" />
            </geometry>
            <material name="black" />
            <origin rpy="0 0 0" xyz="0 0 -0.075" />
        </visual>
    </link>

    <joint name="base_link_joint" type="fixed">
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="0 0 0.171" />
        <parent link="base_footprint" />
        <child link="base_link" />
    </joint>

    <link name="vertical_link">
        <visual>
            <geometry>
                <box size="0.02 0.02 1.025" />
            </geometry>
            <material name="black" />
            <!--<origin rpy="0 0 0" xyz="0 0 0.5125" />-->
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="vertical_joint" type="fixed">
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="-0.035 0 0.5125" />
        <parent link="base_link" />
        <child link="vertical_link" />
    </joint>

    <link name="rod_link">
        <visual>
            <geometry>
                <cylinder length="0.825" radius=".004" />
            </geometry>
            <material name="steel" />
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="rod_joint" type="fixed">
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="0.045 0 0" />
        <parent link="vertical_link" />
        <child link="rod_link" />
    </joint>

    <link name="torso_link">
        <visual>
            <geometry>
                <box size="0.15 0.145 0.005" />
            </geometry>
            <material name="black" />
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="torso_joint" type="prismatic">
        <axis xyz="0 0 1" />
        <limit effort="1000.0" lower="0.21" upper="0.9" velocity="0.0303875" />
        <origin rpy="0 0 0" xyz="0.08 0 -0.5125" />
        <parent link="rod_link" />
        <child link="torso_link" />
    </joint>

    <link name="braccio_base_link">
        <visual>
            <geometry>
                <mesh filename="package://onine_urdf/stl/braccio_base.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <material name="orange" />
            <origin rpy="0 0 1.570796" xyz="0.003 0 -0.075" />
        </visual>
    </link>

    <joint name="base_joint" type="revolute">
        <axis xyz="0 0 1" />
        <limit effort="1000.0" lower="-1.570796" upper="1.4835295" velocity="1.0" />
        <origin rpy="0 0 0" xyz="0 0 0.0756" />
        <parent link="torso_link" />
        <child link="braccio_base_link" />
    </joint>

    <link name="shoulder_link">
        <visual>
            <geometry>
                <mesh filename="package://onine_urdf/stl/braccio_shoulder.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <material name="orange" />
            <origin rpy="0 0 1.570796" xyz="-0.0055 -0.004 -0.026" />
        </visual>
    </link>

    <joint name="shoulder_joint" type="revolute">
        <axis xyz="0 1 0" />
        <limit effort="1000.0" lower="-1.308997" upper="1.308997" velocity="1.0" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <parent link="braccio_base_link" />
        <child link="shoulder_link" />
    </joint>

    <link name="elbow_link">
        <visual>
            <geometry>
                <mesh filename="package://onine_urdf/stl/braccio_elbow.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <material name="orange" />
            <origin rpy="0 0 1.570796" xyz="-0.0055 -0.004 -0.026" />
        </visual>
    </link>

    <joint name="elbow_joint" type="revolute">
        <axis xyz="0 1 0" />
        <limit effort="1000.0" lower="-1.570796" upper="1.570796" velocity="1.0" />
        <origin rpy="0 0 0" xyz="0 0 0.125" />
        <parent link="shoulder_link" />
        <child link="elbow_link" />
    </joint>

    <link name="wrist_pitch_link">
        <visual>
            <geometry>
                <mesh filename="package://onine_urdf/stl/braccio_wrist_pitch.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <material name="orange" />
            <origin rpy="0 0 -1.570796" xyz="0 -0.004 -0.024" />
        </visual>
    </link>

    <joint name="wrist_pitch_joint" type="revolute">
        <axis xyz="0 1 0" />
        <limit effort="1000.0" lower="-1.570796" upper="1.570796" velocity="1.0" />
        <origin rpy="0 0 0" xyz="0 0 0.125" />
        <parent link="elbow_link" />
        <child link="wrist_pitch_link" />
    </joint>

    <link name="wrist_roll_link">
        <visual>
            <geometry>
                <mesh filename="package://onine_urdf/stl/braccio_wrist_roll.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <material name="white" />
            <origin rpy="-1.570796 0 -1.570796" xyz="0 -0.0063 0.001" />
        </visual>
    </link>

    <joint name="wrist_roll_joint" type="revolute">
        <axis xyz="1 0 0" />
        <limit effort="1000.0" lower="0.000" upper="3.141592" velocity="1.0" />
        <origin rpy="0 -1.570796 0" xyz="0 0 0.06" />
        <parent link="wrist_pitch_link" />
        <child link="wrist_roll_link" />
    </joint>

    <link name="tool_link" />

    <joint name="tool_link_joint" type="fixed">
        <axis xyz="1 0 0" />
        <origin rpy="0 0 0" xyz="0.08 0 0" />
        <parent link="wrist_roll_link" />
        <child link="tool_link" />
    </joint>

    <link name="left_gripper_link" >
        <visual>
            <geometry>
                <box size="0.075 0.002 0.006" />
            </geometry>
            <material name="white" />
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="gripper_joint" type="prismatic">
        <axis xyz="0 1 0" />
        <limit effort="1000.0" lower="0.002" upper="0.04" velocity="1.0" />
        <parent link="wrist_roll_link" />
        <origin rpy="0 0 0" xyz=".0776 0 0.005" />
        <child link="left_gripper_link" />
    </joint>

    <link name="right_gripper_link">
        <visual>
            <geometry>
                <box size="0.075 0.002 0.006" />
            </geometry>
            <material name="white" />
            <origin rpy="0 0 0" xyz="0 0 0" />
        </visual>
    </link>

    <joint name="sub_gripper_joint" type="prismatic">
        <axis xyz="0 -1 0" />
        <mimic joint="gripper_joint" />
        <limit effort="1000.0" lower="0.002" upper="0.04" velocity="1.0" />
        <origin rpy="0 0 0" xyz="0.0776 0 0.005" />
        <parent link="wrist_roll_link" />
        <child link="right_gripper_link" />
    </joint>

</robot>